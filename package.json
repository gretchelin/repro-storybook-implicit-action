{
  "name": "storybook-implicit-repro",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc -b ./tsconfig.lib.json",
    "lint:fix": "eslint --fix",
    "format": "prettier --ignore-unknown --write src",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky",
    "test": "vitest run",
    "test:coverage": "vitest --coverage",
    "hygen": "HYGEN_TMPLS=.hygen/_templates hygen",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {},
  "devDependencies": {
    "@eslint/css": "^0.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@microsoft/api-extractor": "^7.53.0",
    "@storybook/addon-docs": "^9.1.13",
    "@storybook/addon-links": "^9.1.13",
    "@storybook/builder-vite": "^9.1.13",
    "@storybook/vue3-vite": "^9.1.13",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node22": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/eslint-plugin": "^1.1.39",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/language-core": "^3.1.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.27.0",
    "eslint-plugin-storybook": "^9.1.13",
    "eslint-plugin-vue": "~10.0.0",
    "globals": "^16.2.0",
    "happy-dom": "^15.7.4",
    "husky": "^9.1.7",
    "hygen": "^6.2.11",
    "jest-extended": "^4.0.2",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.3",
    "postcss-lightningcss": "^1.0.2",
    "postcss-nested": "^7.0.2",
    "prettier": "3.5.3",
    "storybook": "^9.1.13",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.8.0",
    "typescript-eslint": "^8.32.1",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-dts": "1.0.0-beta.0",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "lint-staged": {
    "*.vue": "npm run lint:fix",
    "**/!(node_modules|public|test)/*.{vue,js,jsx,ts,tsx,md,html,css}": "npm run lint:fix"
  },
  "overrides": {
    "storybook": "$storybook"
  },
  "imports": {
    "#tailwind.css": "./src/assets/css/tailwind.css"
  },
  "files": [
    "dist",
    "src/components"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.umd.cjs"
      }
    },
    "./style.css": "./dist/index.css"
  }
}
